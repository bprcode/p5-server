	"item": [
		{
			"name": "Login sample user",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"console.log('trying credentials: ', pm.collectionVariables.get('sample-pass'), pm.collectionVariables.get('sample-userkey'))"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    const json = pm.response.json()\r",
							"    pm.expect(json).to.include({email: pm.collectionVariables.get('sample-userkey')})\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"email\": \"{{sample-userkey}}\",\r\n    \"password\": \"{{sample-pass}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{domain}}/login",
					"host": [
						"{{domain}}"
					],
					"path": [
						"login"
					]
				}
			},
			"response": []
		},

			"name": "Login sample user",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"console.log('trying credentials: ', pm.collectionVariables.get('sample-pass'), pm.collectionVariables.get('sample-userkey'))"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Log in sample user\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    const json = pm.response.json()\r",
							"    pm.expect(json).to.include({email: pm.collectionVariables.get('sample-userkey')})\r",
							"});"
						],
